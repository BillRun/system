#!/usr/bin/expect

set argv1 [lindex $argv 0]
set argv2 [lindex $argv 1]
set argv3 [lindex $argv 2]
spawn sftp billrun@172.29.20.30
expect "password:"
send "$argv3\n"
expect "sftp>"
send "ls $argv1 /data/tmp/International_calls/from_golan\n"
expect "sftp>"
send "put $argv1 /data/tmp/International_calls/from_golan/\n"
expect "sftp>"
send "chgrp 1001 /data/tmp/International_calls/from_golan/$argv2\n"
expect "sftp>"
send "chmod 664 /data/tmp/International_calls/from_golan/$argv2\n"
expect "sftp>"
